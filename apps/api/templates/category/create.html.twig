{% extends 'base.html.twig' %}

{% block title  %}
			Création d'une catégorie
{% endblock %}


{% block body %}
	<div class="container mt-4">
		<h1>Créer une catégorie</h1>

		{{form_start(formView)}}

		<div class="row">
			<div class="col-md-12">{{form_row(formView.label)}}</div>
		</div>

		<button class="btn btn-primary">
			<i class="fas fa-save"></i>
			Enrengistrer
		</button>
		{{form_end(formView)}}
	</div>
{% endblock %}
